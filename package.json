  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "mysql": "^2.17.1",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^8.0.0",
    "typekit": "^0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SANTO/rick/node_express_app"
  },
  "bugs": {
    "url": "https://github.com/SANTO/rick/node_express_app"
